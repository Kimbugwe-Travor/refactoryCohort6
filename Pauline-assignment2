function shoe(color, type, make, owner, size, purpose, origin){
    this.color = color;
    this.type = type;
    this.make = make;
    this.owner = owner;
    this.size = size;
    this.purose = purpose;
    this.origin = origin;
}

var shoe1 = new shoe("Red", "high heels", "Prada", "Fifi", "41", "Office", "Italy")
var shoe2 = new shoe("Black", "Pumps", "Jimmy Choo", "Sheba", "38", "School", "London")

console.log(shoe1);
